package edu.iful.oop;

public class MainClass {

    public static void main(String[] args) {
        Matrix matrix1=new Matrix(3,3); 
        Matrix matrix2=new Matrix(3,3);
        
        System.out.println("Матриця1: ");
        matrix1.printMatrix(matrix1.getMatrix());
        
        System.out.println("Матриця2: ");
        matrix2.printMatrix(matrix2.getMatrix());
        
        System.out.println("Віднімання: ");
        matrix1.printMatrix(matrix1.substractionMatrix(matrix1.getMatrix(), matrix2.getMatrix()));

    }
    }
